{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "dist",
    "backups",
    "repos",
    "node_modules",
    "tmp",
    "e2e",
    "src/commands/ws/configs",
    "package.json"
  ],
  "include": [
    "src"
  ]
}
