{
  "name": "@vlegm/cli",
  "version": "1.0.12",
  "description": "CLI tool for developers",
  "author": "Alfred Cepeda",
  "license": "ISC",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vlm": "./dist/cli.js"
  },
  "scripts": {
    "dev": "npm install -g ./",
    "cli": "ts-node --files src/cli.ts",
    "build": "tsc",
    "generate": "json2ts assets/docker-compose.schema.json > src/docker-compose.ts",
    "test": "mocha --exit --require ts-node/register --require configs/unit.ts src/**/*.specs.ts",
    "e2e": "mocha --require ts-node/register e2e/specs/**/*.specs.ts",
    "test:watch": "mocha -w --watch-extensions ts --require ts-node/register --require configs/unit.ts src/**/*.specs.ts",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "assets",
    "configs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-string": "^1.4.2",
    "@types/ejs": "^3.1.0",
    "@types/inquirer": "^7.3.3",
    "@types/mocha": "^9.0.0",
    "@types/mustache": "^4.1.2",
    "@types/mv": "^2.1.2",
    "@types/node": "^16.7.1",
    "@types/unzipper": "^0.10.5",
    "chai": "^4.3.4",
    "chai-string": "^1.5.0",
    "ejs": "^3.1.6",
    "mocha": "^9.1.3",
    "ts-node": "^10.2.1",
    "tsc": "^2.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@vlegm/utils": "^1.0.1",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "ejs": "^3.1.6",
    "inquirer": "^8.1.2",
    "json-schema-to-typescript": "^10.1.5",
    "lodash.filter": "^4.6.0",
    "mv": "^2.1.1",
    "node-json-db": "^1.3.0",
    "unzipper": "^0.10.11"
  }
}
